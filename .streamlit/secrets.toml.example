# Example Streamlit secrets configuration file
# Rename this file to 'secrets.toml' (remove the .example extension) and fill in your actual API keys
# DO NOT commit the actual secrets.toml file to your repository

[openai]
api_key = "your_openai_api_key"
model = "gpt-4-turbo-preview"

[qdrant]
url = "your_qdrant_url"
api_key = "your_qdrant_api_key"
collection = "math_knowledge"

[exa]
api_key = "your_exa_api_key"

[serper]
api_key = "your_serper_api_key"

[settings]
debug_mode = false
track_performance = true
model_temperature = 0
model_max_tokens = 2048 